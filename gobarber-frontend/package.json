    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",

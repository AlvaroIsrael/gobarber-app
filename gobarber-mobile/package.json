    "date-fns": "^2.23.0",
    "debug": "^4.3.2",
    "metro": "^0.66.2",
    "react": "17.0.2",
    "react-native": "0.64.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-picker": "^4.0.6",
    "react-native-iphone-x-helper": "^1.3.1",
